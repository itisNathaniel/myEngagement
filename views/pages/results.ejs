<html>
<head>
<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/bulma/0.7.4/css/bulma.css">
<link  rel="stylesheet" href="https://unpkg.com/bulma-modal-fx/dist/css/modal-fx.min.css" />
<script type="text/javascript" src="https://unpkg.com/bulma-modal-fx/dist/js/modal-fx.min.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

</head>
<body>

<!-- <%= (unknown[0])[0] %> -->

<div class="">

<!-- top row --> 
<div class="columns">
        <div class="column attendedblock bigblock">
          <span class="keyNumber"><%= sessionAttended.length %></span>
          <span class="subtext">Sessions Attended</span>
          <button class="button blockbutton modal-button" data-target="#attendedModal" aria-haspopup="true">See More</button>
          <%- include ../partials/attended.ejs %>
        </div>
        <div class="column absentblock bigblock">
            <span class="keyNumber"><%= sessionAbsent.length %></span>
            <span class="subtext">Sessions Absent</span>
            <button class="button blockbutton inline modal-button" data-target="#absentModal" aria-haspopup="true">See More</button>
<%- include ../partials/absent.ejs %>

            <span class="keyNumber"><%= authAbsentCount %></span>
            <span class="subtext">of these were informed</span>
            <button class="button blockbutton inline modal-button" data-target="#authAbsentModal" aria-haspopup="true">See More</button>
<%- include ../partials/authAbsent.ejs %>

        </div>
        <div class="column doorAccessblock bigblock">
            <span class="keyNumber"><%= accessGeneral.length + libraryAccess.length + accomodationAccess.length  %></span>
            <span class="subtext">Doors Opened</span>
            <span class="minitext"><%= accessGeneral.length %> Card Access Doors Opened</span>
            <button class="button blockbutton modal-button" data-target="#accessGeneralModal" aria-haspopup="true">See More</button>
            <%- include ../partials/accessGeneral.ejs %>
            <span class="minitext"><%= libraryAccess.length %> Library Turnstiles Opened</span>
            <button class="button blockbutton modal-button" data-target="#accessLibraryModal" aria-haspopup="true">See More</button>
            <%- include ../partials/accessLibrary.ejs %>
            <span class="minitext"><%= accomodationAccess.length %> University Accomodation</span>
            <button class="button blockbutton modal-button" data-target="#accomodationAccessModal" aria-haspopup="true">See More</button>
            <%- include ../partials/accessAccomodation.ejs %>
        </div>
        <div class="column eduroamblock bigblock">
                <span class="keyNumber"><%= wifiLogin.length %></span>
                <span class="subtext">eduroam sign-ins</span>
        </div>
</div>

<!-- second row --> 
<div class="columns">
        <div class="column vleblock bigblock">
                <span class="keyNumber"><%= vleLogin.length + panopto.length + webpa.length  %></span>
                <span class="subtext">VLE Logins</span>  
                <span class="minitext"><%= vleLogin.length %> Canvas Logins</span>  
                <span class="minitext"><%= panopto.length %> Panopto Logins</span>   
                <span class="minitext"><%= webpa.length %> webPA Logins</span>  
        </div>
        <div class="column printblock bigblock">
                <span class="keyNumber"><%= printingGeneral.length + printingScanner.length +  printingCopy.length %></span>
                <span class="subtext">Printers Used</span>  
                <span class="minitext"><%= printingGeneral.length %> Prints Started</span>  
                <span class="minitext"><%= printingScanner.length %> Documents Scanned</span>  
                <span class="minitext"><%= printingCopy.length %> Photocopiers Used</span> 
        </div>
        <div class="column ssoServicesSeatsUnionblock bigblock">
                <span class="keyNumber"><%= ssoSEATS.length %></span>
                <span class="subtext">times you've checked attendance</span>  
                <span class="keyNumber"><%= ssoUnion.length %></span>
                <span class="subtext">times you've used Union services online</span> 
        </div>
        <div class="column ihullblock bigblock">
                <span class="keyNumber"><%= universityApp.length %></span>
                <span class="subtext">times you've logged into iHull</span>  
        </div>
</div>

<!-- third row --> 
<div class="columns">
        <div class="column bigblock">
                <span class="keyNumber"><%= induction.length %></span>
                <span class="subtext">Inductions online</span>  

        </div>
        <div class="column inductionblock bigblock">
                <span class="keyNumber"><%= ssoHousing.length %></span>
                <span class="subtext">Accomodation Portal Uses</span>  
        </div>
        <div class="column bigblock">

        </div>
        <div class="column bigblock">

        </div>
</div>

</div>

<hr>

<button class="button is-primary modal-button" data-target="#unexpectedAttendanceModal" aria-haspopup="true">Unexpected Attendance</button>
<%- include ../partials/unexpectedAttendance.ejs %>

<button class="button is-primary modal-button" data-target="#wifiLoginModal" aria-haspopup="true">wifiLogin</button>
<%- include ../partials/wifiLogin.ejs %>

<button class="button is-primary modal-button" data-target="#ssoLibraryResourceModal" aria-haspopup="true">ssoLibraryResource</button>
<%- include ../partials/ssoLibraryResource.ejs %>

<button class="button is-primary modal-button" data-target="#ssoUnionModal" aria-haspopup="true">ssoUnion</button>
<%- include ../partials/ssoUnion.ejs %>

<button class="button is-primary modal-button" data-target="#vleLoginModal" aria-haspopup="true">vleLogin</button>
<%- include ../partials/ssoVLELogin.ejs %>

<button class="button is-primary modal-button" data-target="#panoptoModal" aria-haspopup="true">panoptoModal</button>
<%- include ../partials/panopto.ejs %>

<button class="button is-primary modal-button" data-target="#printingGeneral" aria-haspopup="true">printingGeneral</button>
<%- include ../partials/printingGeneral.ejs %>

<button class="button is-primary modal-button" data-target="#printingScannerModal" aria-haspopup="true">printingScanner</button>
<%- include ../partials/printingScanner.ejs %>

<button class="button is-primary modal-button" data-target="#printingCopyModal" aria-haspopup="true">printingCopy</button>
<%- include ../partials/printingCopy.ejs %>

<button class="button is-primary modal-button" data-target="#webPAModal" aria-haspopup="true">webPA</button>
<%- include ../partials/ssoWebPA.ejs %>

<button class="button is-primary modal-button" data-target="#ssoGeneralModal" aria-haspopup="true">ssoGeneral</button>
<%- include ../partials/ssoGeneral.ejs %>

<button class="button is-primary modal-button" data-target="#ssoSEATSModal" aria-haspopup="true">ssoSEATS</button>
<%- include ../partials/ssoSEATS.ejs %>

<button class="button is-primary modal-button" data-target="#ssoHousingModal" aria-haspopup="true">ssoHousing</button>
<%- include ../partials/ssoHousing.ejs %>

<button class="button is-primary modal-button" data-target="#ssoUniversityAppModal" aria-haspopup="true">ssoUniversityApp</button>
<%- include ../partials/ssoUniversityApp.ejs %>

<button class="button is-primary modal-button" data-target="#ssoInductionModal" aria-haspopup="true">ssoInduction</button>
<%- include ../partials/ssoInduction.ejs %>

<button class="button is-primary modal-button" data-target="#ssoUnknownModal" aria-haspopup="true">extras</button>
<%- include ../partials/unknown.ejs %>

<script>
document.querySelectorAll('.modal-button').forEach(function(el) {
  el.addEventListener('click', function() {
    var target = document.querySelector(el.getAttribute('data-target'));
    
    target.classList.add('is-active');
    
    target.querySelector('.delete').addEventListener('click',   function() {
        target.classList.remove('is-active');
     });

    target.querySelector('.modal-background').addEventListener('click',   function() {
    target.classList.remove('is-active');
    });

    target.querySelector('.modal-close').addEventListener('click',   function() {
        target.classList.remove('is-active');
     });
  });
});

</script>

<style>
body {
    background-color: #3C185B;
}
.sessionNameWidth {
    max-width: 350px;
    margin-right: 30px;
    display: block;
}

.individualSession td {
    padding: 15px 10px;
}

.individualSessionHead th {
    padding: 10px 10px;
}
.authorised {
    background-color: #f6ab09 !important;
}
.absent {
    background-color: #f8595b;
}

.keyNumber {
    font-size: 35px;
    display: block;
}

.minitext {
    font-size: 12px;
    display: block;
}

.bigblock {
    padding: 30px;
    vertical-align: middle;
    color: #fff;
    min-height: calc(100%/3 + 5px);
    position: relative;
}
.attendedblock {
    background-color: #15d476;;
}
.absentblock {
    background-color: #FF6666;
}
.doorAccessblock {
    background-color: #EDAE49;
}
.eduroamblock {
    background-color: #2C55A2;
}

.vleblock {
    background-color: #587291;
}

.printblock {
    background-color: #3C185B;
}

.ssoServicesSeatsUnionblock {
    background-color: #E54B4B;
}

.ihullblock {
    background-color: #282828;
}

.inductionBlock {
    background-color: #008E9C;
}

.blockbutton {
    opacity: 0.8;
    padding: 0px;
    background: none;
    color: #ffffff;
    border: 0px;
    font-size: 14px;
    border-bottom: 1px solid;
    border-radius: 0px;
    bottom: 10px;
    display: block;
    margin-top: 10px;
    transition: 0.5s ease;
    clear: both !important;
    border-bottom: 0px;
    font-size: 12px;
    position: relative !important;
}

.blockbutton:hover {
    opacity: 0.8;
    border-color: #e6e6e6 !important;
    color: #ffffff !important;
}


</style>

